// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Customer {
  id    Int     @id @default(autoincrement())
  email String? @unique
  name  String
}

model Admin {
  id           Int    @id @default(autoincrement())
  login        String @unique
  name         String
  password     String
  passwordSalt String
}
